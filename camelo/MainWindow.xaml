                0,40,0    0,40,40      100,40,0  100,40,40"
     TriangleIndices=" 0, 2, 1   1, 2, 3
							  4, 5, 6   6, 5, 7                                  
							  8, 9,10   9,11,10                                  
							 12,13,14  12,15,13
							 16,17,18  19,18,17
							 20,21,22  22,21,23">

            </MeshGeometry3D>
            <MeshGeometry3D 
    x:Key="gominho"
    TextureCoordinates="0,0
     0,100
     100,0
     100,100"
    Positions=" 0,0,0    100,0,0        0,50,0     100,50,0
                0,0,0    0,0,100        0,50,0     0,50,100
                0,0,0    100,0,0        0,0,100     100,0,100
                100,0,0   100,50,100    100,0,100   100,50,0
                0,0,100    100,0,100     0,50,100   100,50,100
