        xmlns:sys="clr-namespace:System;assembly=mscorlib"
		xmlns:local="clr-namespace:camelo"
		mc:Ignorable="d"
		Title="MainWindow" Height="450" Width="800">
    <Viewport3D x:Name="t"  Margin="0,0,0,9">

        <Viewport3D.Resources>
