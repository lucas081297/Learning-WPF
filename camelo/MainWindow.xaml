							 16,17,18  19,18,17
							 20,21,22  22,21,23">
            </MeshGeometry3D>
            <MeshGeometry3D x:Key="coxa"
				Positions=" 0, 0, 0  20, 0, 0   0,50, 0  20,50, 0
							0, 0, 0   0, 0,20   0,50, 0   0,50,20
							0, 0, 0  20, 0, 0   0, 0,20  20, 0,20
						   20, 0, 0  20,50,20  20, 0,20  20,50, 0
							0, 0,20  20, 0,20   0,50,20  20,50,20
